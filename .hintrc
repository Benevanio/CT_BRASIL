{
  "extends": [
    "development"
  ],
  "browserslist": [
    "defaults",
    "not ie 11",
    "not ios_saf <= 12.5",
    "not and_ff <= 107",
    "not firefox <= 108",
    "not ios_saf <= 14.8",
    "not opera <= 92",
    "not ios_saf <= 16.2",
    "not safari <= 16.2"
  ],
  "hints": {
    "manifest-file-extension": "off",
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "meta[name=theme-color]"
        ]
      }
    ],
    "apple-touch-icons": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "backdrop-filter"
        ]
      }
    ]
  }
}